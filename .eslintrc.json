{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "plugins": ["import"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "require-jsdoc": 0,
    "import/extensions": [
      1,
      {
        "js": "always",
        "json": "always"
      }
    ]
  },
  "ignorePatterns": ["dist/", "build/"]
}
